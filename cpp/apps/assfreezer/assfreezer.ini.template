[Assfreezer]
ApplicationName=ArsenalFreezer

[Database]
DatabaseDriver=QPSQL7
DatabaseName=blur
Host=pgsql
Options=
Password=yourdbpasswd
Port=5432
Timeout=2
User=yourdbuser

[Display Prefs]
Icon Size=16,16

[Logging]
EchoStdOut=true
Enabled=true
File=/tmp/assfreezer.log
MaxFiles=10
MaxSeverity=3
